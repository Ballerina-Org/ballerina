{
  "types": {
    "User": {
      "fields": {
        "Id": "string",
        "Name": "string",
        "Surname": "string",
        "Birthday": "Date",
        "Email": "string",
        "SubscribeToNewsletter": "boolean",
        "InactiveUsers": {
          "fun": "Table",
          "args": ["User"]
        }
      }
    },
    "UsersSetupConfig": {
      "fields": {}
    },
    "UsersSetup": {
      "fields": {
        "Admin": {
          "fun": "One",
          "args": ["User"]
        },
        "ActiveUsers": {
          "fun": "Table",
          "args": ["User"]
        },
        "InactiveUsers": {
          "fun": "Table",
          "args": ["User"]
        }
      }
    }
  },
  "apis": {
    "enumOptions": {},
    "searchableStreams": {},
    "entities": {
      "UsersSetup": {
        "type": "UsersSetup",
        "methods": ["get"]
      },
      "UsersSetupConfig": {
        "type": "UsersSetupConfig",
        "methods": ["get"]
      }
    },
    "tables": {
      "ActiveUsersApi": {
        "type": "User"
      },
      "InactiveUsersApi": {
        "type": "User"
      }
    }
  },
  "forms": {
    "InactiveUsers": {
      "type": "User",
      "renderer": "streamingTable",
      "columns": {
        "Name": {
          "renderer": "defaultString",
          "disabled": true
        },
        "Surname": {
          "renderer": "defaultString"
        }
      },
      "visibleColumns": ["Name", "Surname"]
    },
    "ActiveUsers": {
      "type": "User",
      "renderer": "streamingTable",
      "columns": {
        "Name": {
          "renderer": "defaultString"
        },
        "Surname": {
          "renderer": "defaultString",
          "disabled": true
        }
      },
      "detailsRenderer": {
        "type": "User",
        "renderer": "userDetails",
        "fields": {
          "Name": {
            "label": "First Name",
            "renderer": "defaultString"
          },
          "Surname": {
            "label": "Last Name",
            "renderer": "defaultString",
            "disabled": true
          },
          "Email": {
            "label": "Email",
            "renderer": "defaultString"
          },
          "Birthday": {
            "label": "Birthday",
            "renderer": "defaultDate"
          },
          "SubscribeToNewsletter": {
            "label": "Subscribe to Newsletter",
            "renderer": "defaultBoolean"
          },
          "InactiveUsers": {
            "label": "Active Users",
            "renderer": "InactiveUsers",
            "api": "ActiveUsersApi"
          }
        },
        "tabs": {
          "main": {
            "columns": {
              "main": {
                "groups": {
                  "main": [
                    "Name",
                    "Surname",
                    "Email",
                    "Birthday",
                    "SubscribeToNewsletter",
                    "InactiveUsers"
                  ]
                }
              }
            }
          }
        }
      },
      "visibleColumns": ["Name", "Surname"]
    },
    "UsersSetup": {
      "type": "UsersSetup",
      "renderer": "userDetails",
      "fields": {
        "ActiveUsers": {
          "renderer": "ActiveUsers",
          "api": "ActiveUsersApi",
          "label": "Active Users"
        },
        "Admin": {
          "renderer": "admin",
          "detailsRenderer": {
            "type": "User",
            "renderer": "userDetails",
            "fields": {
              "Name": {
                "label": "First Name",
                "renderer": "defaultString"
              },
              "Surname": {
                "label": "Last Name",
                "renderer": "defaultString"
              }
            },
            "tabs": {
              "main": {
                "columns": {
                  "main": {
                    "groups": {
                      "main": ["Name", "Surname"]
                    }
                  }
                }
              }
            }
          },
          "api": "AdminApi",
          "label": "Admin"
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": ["ActiveUsers", "Admin"]
              }
            }
          }
        }
      }
    }
  },
  "launchers": {
    "UsersSetupLauncher": {
      "kind": "passthrough",
      "form": "UsersSetup",
      "configType": "UsersSetupConfig"
    }
  }
}
