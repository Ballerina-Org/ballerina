{
  "types": {
    "GlobalConfiguration": {
      "extends": [],
      "fields": {}
    },
    "ERPConfig": {
      "fun": "Union",
      "args": [
        {
          "caseName": "ERPSAP",
          "fields": {
            "Value": "SAPConfig"
          }
        },
        {
          "caseName": "ERPBC",
          "fields": {}
        },
        {
          "caseName": "ERPFAndO",
          "fields": {}
        }
      ]
    },
    "SAPConfig": {
      "fun": "Union",
      "args": [
        {
          "caseName": "SAPS2",
          "fields": {
            "S2OnlyField": "boolean"
          }
        },
        {
          "caseName": "SAPS3",
          "fields": {
            "S3OnlyField": "boolean"
          }
        },
        {
          "caseName": "SAPS4",
          "fields": {
            "S4OnlyField": "boolean"
          }
        }
      ]
    },
    "CollectionReference": {
      "extends": [],
      "fields": {
        "DisplayValue": "string",
        "Id": "guid"
      }
    },
    "User": {
      "fields": {
        "Id": "string",
        "Name": "string",
        "Surname": "string",
        "Category": "injectedCategory"
      }
    },
    "CityRef": {
      "extends": ["CollectionReference"],
      "fields": {}
    },
    "Address": {
      "fields": {
        "Street": "string",
        "Town": "string",
        "Number": "number",
        "Category": "injectedCategory",
        "SumExample": {
          "fun": "Sum",
          "args": ["unit", "injectedCategory"]
        },
        "TupleExample": {
          "fun": "Tuple",
          "args": ["injectedCategory", "injectedCategory", "injectedCategory"]
        }
      }
    },
    "ColorsEnum": {
      "fun": "Union",
      "args": [
        {
          "caseName": "Red",
          "fields": {}
        },
        {
          "caseName": "Green",
          "fields": {}
        },
        {
          "caseName": "Blue",
          "fields": {}
        }
      ]
    },
    "ColorRef": {
      "fields": {
        "Value": "ColorsEnum"
      }
    },
    "InterestsEnum": {
      "fun": "Union",
      "args": [
        {
          "caseName": "Soccer",
          "fields": {}
        },
        {
          "caseName": "Hockey",
          "fields": {}
        },
        {
          "caseName": "BoardGames",
          "fields": {}
        },
        {
          "caseName": "HegelianPhilosophy",
          "fields": {}
        }
      ]
    },
    "InterestRef": {
      "fields": {
        "Value": "InterestsEnum"
      }
    },
    "DepartmentRef": {
      "extends": ["CollectionReference"],
      "fields": {}
    },
    "BaseJob": {
      "fields": {
        "Name": "string",
        "Salary": "number"
      }
    },
    "DeveloperJob": {
      "extends": ["BaseJob"],
      "fields": {
        "Language": "string",
        "Category": "injectedCategory"
      }
    },
    "DesignerJob": {
      "extends": ["BaseJob"],
      "fields": {
        "DesignTool": "string",
        "Certifications": {
          "fun": "List",
          "args": ["string"]
        },
        "Category": "injectedCategory"
      }
    },
    "Job": {
      "fun": "Union",
      "args": [
        {
          "caseName": "Developer",
          "fields": "DeveloperJob"
        },
        {
          "caseName": "Designer",
          "fields": "DesignerJob"
        }
      ]
    },
    "Person": {
      "fields": {
        "Job": "Job",
        "Friends": {
          "fun": "Table",
          "args": ["User"]
        },
        "Name": "string",
        "Category": "injectedCategory",
        "Birthday": "Date",
        "SubscribeToNewsletter": "boolean",
        "SchoolAddress": "Address",
        "CategorySum": {
          "fun": "Sum",
          "args": ["unit", "injectedCategory"]
        },
        "Relatives": {
          "fun": "Tuple",
          "args": ["string", "string", "injectedCategory"]
        }
      }
    }
  },
  "apis": {
    "enumOptions": {
      "colors": "ColorRef",
      "interests": "InterestRef"
    },
    "searchableStreams": {
      "cities": "CityRef",
      "departments": "DepartmentRef"
    },
    "entities": {
      "person": {
        "type": "Person",
        "methods": ["create", "get", "update", "default"]
      },
      "globalConfiguration": {
        "type": "GlobalConfiguration",
        "methods": ["get"]
      }
    },
    "tables": {
      "ActiveUsersApi": {
        "type": "User"
      }
    }
  },
  "forms": {
    "address": {
      "type": "Address",
      "renderer": "address",
      "fields": {
        "Street": {
          "label": "street",
          "renderer": "defaultString",
          "visible": true
        },
        "Town": {
          "renderer": "defaultString"
        },
        "Category": {
          "renderer": "defaultCategory"
        },
        "Number": { "renderer": "defaultNumber" },
        "SumExample": {
          "renderer": "defaultSum",
          "leftRenderer": { "renderer": "defaultUnit" },
          "rightRenderer": { "renderer": "defaultCategory" }
        },
        "TupleExample": {
          "renderer": "defaultTuple3",
          "visible": {
            "kind": "equals",
            "operands": [
              {
                "kind": "fieldLookup",
                "operands": [
                  { "kind": "varLookup", "varName": "local" },
                  "Street"
                ]
              },
              "Bahnhofstrasse"
            ]
          },
          "itemRenderers": [
            { "renderer": "defaultCategory" },
            { "renderer": "defaultCategory" },
            { "renderer": "defaultCategory" }
          ]
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": [
                  "Street",
                  "Town",
                  "Number",
                  "Category",
                  "SumExample",
                  "TupleExample"
                ]
              }
            }
          }
        }
      }
    },
    "developerJob": {
      "type": "DeveloperJob",
      "renderer": "personDetails",
      "fields": {
        "Name": {
          "renderer": "defaultString"
        },
        "Salary": {
          "renderer": "defaultNumber"
        },
        "Language": {
          "renderer": "defaultString"
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": ["Name", "Salary", "Language"]
              }
            }
          }
        }
      }
    },
    "designerJob": {
      "type": "DesignerJob",
      "renderer": "personDetails",
      "fields": {
        "Name": {
          "renderer": "defaultString"
        },
        "Salary": {
          "renderer": "defaultNumber"
        },
        "DesignTool": {
          "renderer": "defaultString"
        },
        "Certifications": {
          "renderer": "defaultList",
          "elementRenderer": {
            "renderer": "defaultString"
          }
        },
        "Category": {
          "renderer": "defaultCategory"
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": [
                  "Name",
                  "Salary",
                  "DesignTool",
                  "Certifications",
                  "Category"
                ]
              }
            }
          }
        }
      }
    },
    "job": {
      "type": "Job",
      "renderer": "job",
      "cases": {
        "Developer": {
          "renderer": {
            "type": "DeveloperJob",
            "fields": {
              "Name": {
                "renderer": "defaultString"
              },
              "Salary": {
                "renderer": "defaultNumber"
              },
              "Language": {
                "renderer": "defaultString"
              },
              "Category": {
                "renderer": "defaultCategory"
              }
            },
            "tabs": {
              "main": {
                "columns": {
                  "main": {
                    "groups": {
                      "main": ["Name", "Salary", "Language", "Category"]
                    }
                  }
                }
              }
            }
          }
        },
        "Designer": "designerJob"
      }
    },
    "person": {
      "type": "Person",
      "renderer": "personDetails",
      "fields": {
        "Friends": {
          "renderer": {
            "type": "User",
            "renderer": "streamingTable",
            "columns": {
              "Name": {
                "renderer": "defaultString"
              },
              "Category": {
                "renderer": "defaultCategory"
              }
            },
            "visibleColumns": [
              "Name",
              "Category"
            ],
            "detailsRenderer": {
              "renderer": {
                "type": "User",
                "renderer": "userDetails",
                "fields": {
                  "Name": {
                    "label": "First Name",
                    "renderer": "defaultString"
                  },
                  "Surname": {
                    "label": "Last Name",
                    "renderer": "defaultString"
                  },
                  "Category": {
                    "renderer": "defaultCategory"
                  }
                },
                "tabs": {
                  "main": {
                    "columns": {
                      "main": {
                        "groups": {
                          "main": ["Name", "Surname", "Category"]
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "api": "ActiveUsersApi"
        },
        "Job": {
          "label": "Job",
          "renderer": "job"
        },
        "Name": {
          "renderer": "defaultString"
        },
        "Category": {
          "renderer": "defaultCategory"
        },
        "Birthday": {
          "renderer": "defaultDate",
          "tooltip": "happy Birthday!"
        },
        "SubscribeToNewsletter": {
          "label": "subscribe to newsletter",
          "renderer": "defaultBoolean"
        },
        "SchoolAddress": {
          "label": "school address",
          "renderer": "address"
        },
        "CategorySum": {
          "label": "category or string",
          "renderer": "defaultSum",
          "leftRenderer": { "renderer": "defaultUnit" },
          "rightRenderer": { "renderer": "defaultCategory" }
        },
        "Relatives": {
          "label": "relatives",
          "renderer": "defaultTuple3",
          "itemRenderers": [
            { "renderer": "defaultString" },
            { "renderer": "defaultString" },
            { "renderer": "defaultCategory" }
          ]
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "demographics": {
              "groups": {
                "main": [
                  "Job",
                  "Friends",
                  "Name",
                  "Category",
                  "SubscribeToNewsletter",
                  "Birthday",
                  "SchoolAddress",
                  "CategorySum",
                  "Relatives"
                ]
              }
            }
          }
        }
      }
    }
  },
  "launchers": {
    "person-transparent": {
      "kind": "passthrough",
      "form": "person",
      "configType": "GlobalConfiguration"
    }
  }
}
