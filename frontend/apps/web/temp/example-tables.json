{
  "types": {
    "User": {
      "fields": {
        "Id": "string",
        "Name": "string",
        "Surname": "string",
        "Birthday": "Date",
        "Email": "string",
        "SubscribeToNewsletter": "boolean",
        "Job": {
          "fun": "One",
          "args": ["Job"]
        }
      }
    },
    "Job": {
      "fields": {
        "Id": "string",
        "Name": "string",
        "Description": "string"
      }
    },
    "UsersSetupConfig": {
      "fields": {}
    },
    "UsersSetup": {
      "fields": {
        "ActiveUsers": {
          "fun": "Table",
          "args": ["User"]
        },
        "InactiveUsers": {
          "fun": "Table",
          "args": ["User"]
        }
      }
    }
  },
  "apis": {
    "enumOptions": {},
    "searchableStreams": {},
    "entities": {
      "UsersSetup": {
        "type": "UsersSetup",
        "methods": ["get"]
      },
      "UsersSetupConfig": {
        "type": "UsersSetupConfig",
        "methods": ["get"]
      }
    },
    "lookups": {
      "User": {
        "one": {
          "UserJobApi": {
            "type": "Job",
            "methods": ["update", "getManyUnlinked", "create", "delete"]
          }
        }
      }
    },
    "tables": {
      "ActiveUsersApi": {
        "type": "User"
      },
      "InactiveUsersApi": {
        "type": "User"
      }
    }
  },
  "forms": {
    "InactiveUsers": {
      "type": "User",
      "renderer": "table",
      "columns": {
        "Name": {
          "renderer": "defaultString",
          "disabled": true
        },
        "Surname": {
          "renderer": "defaultString"
        }
      },
      "visibleColumns": ["Name", "Surname"]
    },
    "ActiveUsers": {
      "type": "User",
      "renderer": "streamingTable",
      "columns": {
        "Name": {
          "renderer": "defaultString"
        },
        "Surname": {
          "renderer": "defaultString",
          "disabled": true
        }
      },
      "detailsRenderer": {
        "type": "User",
        "fields": {
          "Name": {
            "label": "First Name",
            "renderer": "defaultString"
          },
          "Surname": {
            "label": "Last Name",
            "renderer": "defaultString",
            "disabled": true
          },
          "Email": {
            "label": "Email",
            "renderer": "defaultString"
          },
          "Birthday": {
            "label": "Birthday",
            "renderer": "defaultDate"
          },
          "SubscribeToNewsletter": {
            "label": "Subscribe to Newsletter",
            "renderer": "defaultBoolean"
          },
          "Job": {
            "renderer": "one",
            "detailsRenderer": {
              "renderer": {
                "type": "Job",
                "renderer": "alternatingBackgroundColors",
                "fields": {
                  "Name": {
                    "renderer": "string",
                    "label": "Name"
                  },
                  "Description": {
                    "renderer": "string",
                    "label": "Description"
                  },
                  "Salary": {
                    "renderer": "float64",
                    "label": "Salary"
                  },
                  "Company": {
                    "renderer": "streamSingleselection",
                    "stream": ["User", "UserCompaniesApi"],
                    "label": "Company"
                  }
                },
                "tabs": {
                  "main": {
                    "columns": {
                      "main": {
                        "groups": {
                          "main": ["Name", "Description", "Salary", "Company"]
                        }
                      }
                    }
                  }
                }
              }
            },
            "previewRenderer": {
              "renderer": {
                "type": "Job",
                "fields": {
                  "Name": {
                    "renderer": "string",
                    "label": "Name"
                  }
                },
                "tabs": {
                  "main": {
                    "columns": {
                      "main": {
                        "groups": {
                          "main": ["Name"]
                        }
                      }
                    }
                  }
                }
              }
            },
            "api": ["User", "UserJobApi"],
            "label": "Job"
          }
        },
        "tabs": {
          "main": {
            "columns": {
              "main": {
                "groups": {
                  "main": [
                    "Name",
                    "Surname",
                    "Email",
                    "Birthday",
                    "SubscribeToNewsletter",
                    "Job"
                  ]
                }
              }
            }
          }
        }
      },
      "visibleColumns": ["Name", "Surname"]
    },
    "UsersSetup": {
      "type": "UsersSetup",
      "fields": {
        "ActiveUsers": {
          "renderer": "ActiveUsers",
          "api": "ActiveUsersApi",
          "label": "Active Users"
        },
        "InactiveUsers": {
          "renderer": "InactiveUsers",
          "api": "InactiveUsersApi",
          "label": "Inactive Users"
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": ["ActiveUsers", "InactiveUsers"]
              }
            }
          }
        }
      }
    }
  },
  "launchers": {
    "UsersSetupLauncher": {
      "kind": "passthrough",
      "form": "UsersSetup",
      "configType": "UsersSetupConfig"
    }
  }
}
