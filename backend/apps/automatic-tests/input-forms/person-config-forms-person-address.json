{
  "forms": {
    "address": {
      "type": "Address",
      "fields": {
        "streetNumberAndCity": {
          "label": "street number and city",
          "renderer": "defaultTuple3",
          "itemRenderers": [
            {
              "renderer": "defaultString"
            },
            {
              "renderer": "defaultNumber"
            },
            {
              "renderer": "defaultInfiniteStream",
              "stream": "cities"
            }
          ],
          "visible": {
            "kind": "or",
            "operands": [
              {
                "kind": "fieldLookup",
                "operands": [
                  {
                    "kind": "varLookup",
                    "varName": "root"
                  },
                  "subscribeToNewsletter"
                ]
              },
              {
                "kind": "equals",
                "operands": [
                  {
                    "kind": "itemLookup",
                    "operands": [
                      {
                        "kind": "fieldLookup",
                        "operands": [
                          {
                            "kind": "varLookup",
                            "varName": "local"
                          },
                          "streetNumberAndCity"
                        ]
                      },
                      2
                    ]
                  },
                  10
                ]
              }
            ]
          }
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": [
                  "streetNumberAndCity"
                ]
              }
            }
          }
        }
      }
    },
    "person": {
      "type": "Person",
      "fields": {
        "category": {
          "label": "category",
          "renderer": "defaultCategory"
        },
        "fullName": {
          "label": "full name",
          "renderer": "defaultTuple2",
          "itemRenderers": [
            {
              "renderer": "defaultString"
            },
            {
              "renderer": "defaultString"
            }
          ]
        },
        "job": {
          "renderer": "job"
        },
        "birthday": {
          "renderer": "defaultDate",
          "tooltip": "happy birthday!"
        },
        "favoriteColor": {
          "renderer": "defaultEnum",
          "options": "colors"
        },
        "gender": {
          "label": "gender",
          "renderer": "defaultEnum",
          "options": "genders",
          "visible": {
            "kind": "matchCase",
            "operands": [
              {
                "kind": "fieldLookup",
                "operands": [
                  {
                    "kind": "varLookup",
                    "varName": "global"
                  },
                  "ERP"
                ]
              },
              {
                "caseName": "ERP:SAP",
                "handler": {
                  "kind": "lambda",
                  "parameter": "sapFields",
                  "body": {
                    "kind": "matchCase",
                    "operands": [
                      {
                        "kind": "fieldLookup",
                        "operands": [
                          {
                            "kind": "varLookup",
                            "varName": "sapFields"
                          },
                          "Value"
                        ]
                      },
                      {
                        "caseName": "SAP:S2",
                        "handler": {
                          "kind": "lambda",
                          "parameter": "s2Fields",
                          "body": {
                            "kind": "fieldLookup",
                            "operands": [
                              {
                                "kind": "varLookup",
                                "varName": "s2Fields"
                              },
                              "S2OnlyField"
                            ]
                          }
                        }
                      },
                      {
                        "caseName": "SAP:S3",
                        "handler": {
                          "kind": "lambda",
                          "parameter": "s3Fields",
                          "body": {
                            "kind": "fieldLookup",
                            "operands": [
                              {
                                "kind": "varLookup",
                                "varName": "s3Fields"
                              },
                              "S3OnlyField"
                            ]
                          }
                        }
                      },
                      {
                        "caseName": "SAP:S4",
                        "handler": {
                          "kind": "lambda",
                          "parameter": "s4Fields",
                          "body": {
                            "kind": "fieldLookup",
                            "operands": [
                              {
                                "kind": "varLookup",
                                "varName": "s4Fields"
                              },
                              "S4OnlyField"
                            ]
                          }
                        }
                      }
                    ]
                  }
                }
              },
              {
                "kind": "caseName",
                "caseName": "ERP:BC",
                "handler": {
                  "kind": "lambda",
                  "parameter": "_",
                  "body": false
                }
              },
              {
                "kind": "caseName",
                "caseName": "ERP:FAndO",
                "handler": {
                  "kind": "lambda",
                  "parameter": "_",
                  "body": false
                }
              }
            ]
          }
        },
        "dependants": {
          "label": "dependants",
          "renderer": "defaultMap",
          "tooltip": "someone who depends on you",
          "keyRenderer": {
            "label": "name",
            "tooltip": "their name",
            "renderer": "defaultString"
          },
          "valueRenderer": {
            "label": "category",
            "tooltip": "their category",
            "renderer": "defaultCategory"
          }
        },
        "friendsByCategory": {
          "label": "friends by category",
          "renderer": "defaultMap",
          "keyRenderer": {
            "label": "category",
            "renderer": "defaultCategory"
          },
          "valueRenderer": {
            "label": "name",
            "renderer": "defaultString"
          }
        },
        "relatives": {
          "label": "relatives",
          "tooltip": "someone who you are related to",
          "renderer": "defaultList",
          "elementRenderer": {
            "label": "one relative",
            "tooltip": "relative",
            "renderer": "defaultCategory"
          }
        },
        "subscribeToNewsletter": {
          "label": "subscribe to newsletter",
          "renderer": "defaultBoolean"
        },
        "interests": {
          "label": "interests",
          "renderer": "defaultEnumMultiselect",
          "options": "interests",
          "visible": {
            "kind": "fieldLookup",
            "operands": [
              {
                "kind": "varLookup",
                "varName": "root"
              },
              "subscribeToNewsletter"
            ]
          }
        },
        "departments": {
          "label": "departments",
          "renderer": "defaultInfiniteStreamMultiselect",
          "stream": "departments",
          "disabled": {
            "kind": "fieldLookup",
            "operands": [
              {
                "kind": "varLookup",
                "varName": "local"
              },
              "subscribeToNewsletter"
            ]
          }
        },
        "mainAddress": {
          "label": "main address",
          "renderer": "defaultSum",
          "leftRenderer": {
            "label": "as string",
            "renderer": "defaultString"
          },
          "rightRenderer": {
            "label": "as address",
            "renderer": "defaultTuple2",
            "itemRenderers": [
              {
                "renderer": "address"
              },
              {
                "renderer": "plotInfo"
              }
            ]
          },
          "visible": {
            "kind": "fieldLookup",
            "operands": [
              {
                "kind": "varLookup",
                "varName": "local"
              },
              "subscribeToNewsletter"
            ]
          }
        },
        "addressesBy": {
          "label": "addresses by",
          "renderer": "defaultSum",
          "leftRenderer": {
            "label": "by color",
            "renderer": "defaultMap",
            "keyRenderer": {
              "label": "address color",
              "renderer": "defaultEnum",
              "options": "colors"
            },
            "valueRenderer": {
              "label": "address",
              "renderer": "address"
            }
          },
          "rightRenderer": {
            "label": "by label",
            "renderer": "defaultMap",
            "keyRenderer": {
              "label": "address label",
              "renderer": "defaultString"
            },
            "valueRenderer": {
              "label": "address",
              "renderer": "address"
            }
          }
        },
        "importantDate": {
          "label": "important date",
          "renderer": "maybeDate",
          "visible": true,
          "disabled": false
        },
        "emails": {
          "label": "emails",
          "renderer": "defaultList",
          "elementRenderer": {
            "label": "email",
            "renderer": "defaultString"
          }
        },
        "addressesAndAddressesWithLabel": {
          "label": "other addresses",
          "renderer": "defaultTuple2",
          "itemRenderers": [
            {
              "renderer": "defaultList",
              "elementRenderer": {
                "label": "address",
                "renderer": "address",
                "disabled": {
                  "kind": "equals",
                  "operands": [
                    {
                      "kind": "fieldLookup",
                      "operands": [
                        {
                          "kind": "varLookup",
                          "varName": "root"
                        },
                        "category"
                      ]
                    },
                    "child"
                  ]
                }
              }
            },
            {
              "renderer": "defaultMap",
              "keyRenderer": {
                "label": "address label",
                "renderer": "defaultString"
              },
              "valueRenderer": {
                "label": "address",
                "renderer": "address"
              }
            }
          ]
        },
        "addressesByCity": {
          "label": "addresses by city",
          "renderer": "defaultMap",
          "keyRenderer": {
            "label": "city",
            "tooltip": "a nice place to live",
