          "valueRenderer": {
            "label": "address",
            "renderer": "address"
          }
        },
        "permissions": {
          "label": "permissions",
          "renderer": "defaultMap",
          "keyRenderer": {
            "label": "permission",
            "renderer": "defaultEnum",
            "options": "permissions"
          },
          "valueRenderer": {
            "label": "granted",
            "renderer": "defaultBoolean"
          }
        },
        "cityByDepartment": {
          "label": "city by department",
          "renderer": "defaultMap",
          "keyRenderer": {
            "label": "department",
