      "tabs": {
        "main": {
          "columns": {
            "demographics": {
              "groups": {
                "main": [
                  "category",
                  "fullName",
                  "birthday",
                  "gender",
                  "emails",
                  "dependants",
                  "friendsByCategory",
                  "relatives",
