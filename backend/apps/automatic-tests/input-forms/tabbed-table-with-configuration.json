          "fun": "Option",
          "args": [
            {
              "fun": "One",
              "args": [
                "Job"
              ]
            }
          ]
        },
        "Team": {
          "fun": "One",
          "args": [
            "Team"
          ]
        },
        "UserGroups": {
          "fun": "Many",
          "args": [
            "UserGroup"
          ]
        },
        "Activities": {
          "fun": "Many",
          "args": [
            "Activity"
          ]
        }
      }
    },
    "Users": {
      "fields": {
        "Active": {
          "fun": "Table",
          "args": [
            "User"
          ]
        },
        "Inactive": {
          "fun": "Table",
          "args": [
            "User"
          ]
        }
      }
