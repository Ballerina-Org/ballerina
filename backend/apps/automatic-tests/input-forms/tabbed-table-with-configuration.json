{
  "types": {
    "GlobalConfig": {
      "fields": {}
    },
    "UserGroup": {
      "fields": {
        "Id": "string",
        "Name": "string",
        "Description": "string"
      }
    },
    "Activity": {
      "fields": {
        "Id": "string",
        "Description": "string",
        "Timestamp": "Date"
      }
    },
    "User": {
      "fields": {
        "Id": "string",
        "Name": "string",
        "Surname": "string",
        "Birthday": "Date",
        "Email": "string",
        "Team":{
          "fun": "OptionLookup",
          "args": ["Team"]
        },
        "UserGroups":{
          "fun": "TableLookup",
          "args": [
            "UserGroup"
          ]
        },
        "Activities": {
          "fun": "TableLookup",
          "args": [
            "Activity"
          ]
        }
      }
    },
    "Team": {
      "fields": {
        "Name":"string",
        "Description":"string"
      }
    },
    "UsersSetup": {
      "fields": {
        "Active": {
          "fun": "Table",
          "args": [
            "User"
          ]
        },
        "Inactive": {
          "fun": "Table",
          "args": [
            "User"
          ]
        }
      }
    }
  },
  "apis": {
    "enumOptions": {
    },
    "entities": {
      "GlobalConfigApi": {
        "type": "GlobalConfig",
        "methods": [
          "get"
        ]
      },
      "UsersSetupApi": {
        "type": "UsersSetup",
        "methods": [
          "get",
          "update"
        ]
      }
    },
    "tables": {
      "ActiveUsersApi": {
        "type": "User"
      },
      "InactiveUsersApi": {
        "type": "User"
      }
    },
    "lookups":{
      "User": {
        "tables":{
          "UserGroupsApi": {
            "type": "UserGroup"
          },
          "ActivitiesApi": {
            "type": "Activity"
          }
        },
        "entities": {
          "TeamApi": {
            "type": "Team",
            "methods": [
              "get", "update", "create", "delete"
            ]
          }
        }
      }
    }
  },
  "forms": {
    "activeUsers": {
      "type": "User",
      "renderer": "finiteTable",
      "columns": {
        "Name": {
          "renderer": "string"
        },
        "Surname": {
          "renderer": "string"
        },
        "Email": {
          "renderer": "string"
        }
      },
      "detailView":{ 
        "columns": {
          "Birthday": {
            "renderer": "date"
          },
          "UserGroups":{
            "renderer": "userGroups",
            "detailApi": ["User", "tables", "UserGroupsApi"]
          },
          "Team":{
            "renderer": "optionLookup",
            "elementRenderer": "team",
            "detailApi": ["User", "entities", "TeamApi"]
          },
          "Activities": {
            "renderer": "activities",
            "detailApi": ["User", "tables", "ActivitiesApi"]
          },
          "visibleColumns": ["Birthday", "UserGroups", "Team", "Activities"]
       }
      },
      "visibleColumns": ["Name", "Surname", "Birthday", "Email"]
    },
    "inactiveUsers": {
      "type": "User",
      "renderer": "streamingTable",
      "columns": {
        "Name": {
          "renderer": "string"
        },
        "Surname": {
          "renderer": "string"
        },
        "Birthday": {
          "renderer": "date"
        },
        "Email": {
          "renderer": "string"
        }
      },
      "visibleColumns": ["Name", "Surname", "Birthday", "Email"]
    },
    "userGroups": {
      "type": "UserGroup",
      "renderer": "streamingTable",
      "columns": {
        "Name": {
          "renderer": "string"
        },
        "Description": {
          "renderer": "string"
        }
      },
      "visibleColumns": ["Name", "Description"]
    },
    "activities": {
      "type": "Activity",
      "renderer": "streamingTable",
      "columns": {
        "Description": {
          "renderer": "string"
        },
        "Timestamp": {
          "renderer": "date"
        }
      },
      "visibleColumns": ["Description", "Timestamp"]
    },
    "team": {
      "type": "Team",
      "fields": {
        "Name": {
          "renderer": "string"
        },
        "Description": {
          "renderer": "string"
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": [
                  "Name",
                  "Description"
                ]
              }
            }
          }
        }
      }
    },
    "usersSetup": {
      "type": "UsersSetup",
      "fields": {
        "Active": {
          "renderer": "activeUsers",
          "api": "ActiveUsersApi"
          },
        "Inactive": {
          "renderer": "inactiveUsers",
          "api": "InactiveUsersApi"
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": [
                  "Active",
                  "Inactive"
                ]
              }
            }
          }
        }
      }
    }
  },
  "launchers": {
    "UsersSetup": {
      "kind": "passthrough",
      "form": "UsersSetup",
      "configType": "GlobalConfig"
    }
  }
}