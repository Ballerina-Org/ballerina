{
  "types": {
    "GlobalConfiguration": {
      "fields": {}
    },
    "CollectionReference": {
      "extends": [],
      "fields": {
        "DisplayValue": "string",
        "Id": "guid"
      }
    },
    "CompanyRef": {
      "extends": [
        "CollectionReference"
      ],
      "fields": {}
    },
    "UserGroup": {
      "fields": {
        "Id": "guid",
        "Name": "string",
        "Description": "string"
      }
    },
    "Job": {
      "fields": {
        "Id": "guid",
        "Name": "string",
        "Description": "string",
        "Salary": "Float64",
        "Company": {
          "fun": "SingleSelection",
          "args": [
            "CompanyRef"
          ]
        }
      }
    },
    "Team": {
      "fields": {
        "Id": "guid",
        "Name": "string",
        "Description": "string"
      }
    },
    "Activity": {
      "const": true,
      "fields": {
        "Id": "guid",
        "Log": "string",
        "Timestamp": "Date"
      }
    },
    "Role": {
      "fun": "Union",
      "args": [
        {
          "caseName": "SuperAdmin",
          "fields": {}
        },
        {
          "caseName": "Admin",
          "fields": {}
        },
        {
          "caseName": "Regular",
          "fields": {}
        }
      ]
    },
    "User": {
      "fields": {
        "Id": "guid",
        "Name": "string",
        "Surname": "string",
        "Email": "string",
        "Role": "Role",
        "Job": {
          "fun": "Option",
          "args": [
            {
              "fun": "One",
              "args": [
                "Job"
              ]
            }
          ]
        },
        "Team": {
          "fun": "One",
          "args": [
            "Team"
          ]
        },
        "UserGroups": {
          "fun": "Many",
          "args": [
            "UserGroup"
          ]
        },
        "Activities": {
          "fun": "Many",
          "args": [
            "Activity"
          ]
        }
      }
    },
    "Users": {
      "fields": {
        "Active": {
          "fun": "Table",
          "args": [
            "User"
          ]
        },
        "Inactive": {
          "fun": "Table",
          "args": [
            "User"
          ]
        }
      }
    }
  },
  "apis": {
    "searchableStreams": {},
    "tables": {
      "ActiveUsersApi": {
        "type": "User"
      },
      "InactiveUsersApi": {
        "type": "User"
      }
    },
    "lookups": {
      "User": {
        "streams": {
          "UserCompaniesApi": "CompanyRef"
        },
        "one": {
          "UserTeamApi": {
            "type": "Team",
            "methods": [
              "get",
              "update"
            ]
          },
          "UserJobApi": {
            "type": "Job",
            "methods": [
              "update",
              "getManyUnlinked",
              "create",
              "delete"
            ]
          }
        },
        "many": {
          "UserGroupsApi": {
            "type": "UserGroup",
            "methods": [
              "update",
              "getManyUnlinked",
              "create",
              "delete"
            ]
          },
          "UserActivitiesApi": {
            "type": "Activity",
            "methods": [
              "update",
              "getManyUnlinked",
              "create",
              "delete"
            ]
          }
        }
      }
    }
  },
  "forms": {
    "team": {
      "type": "Team",
      "containerRenderer": "grayBackground",
      "fields": {
        "Name": {
          "renderer": "string",
          "label": "Name"
        },
        "Description": {
          "renderer": "string",
          "label": "Description"
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": [
                  "Name",
                  "Description"
                ]
              }
            }
          }
        }
      }
    },
    "inactiveUsers": {
      "type": "User",
      "renderer": "table",
      "columns": {
        "Email": {
          "renderer": "string",
          "label": "Email"
        }
      },
      "visibleColumns": [
        "Email"
      ]
    },
    "activeUsers": {
      "type": "User",
      "renderer": "table",
      "containerRenderer": "highlighted",
      "columns": {
        "Email": {
          "renderer": "string",
          "label": "Email",
          "disabled": true
        }
      },
      "detailsRenderer": {
        "containerRenderer": "highlighted",
        "fields": {
          "Name": {
            "renderer": "string",
            "label": "Name",
            "disabled": true
          },
          "Surname": {
            "renderer": "string",
            "label": "Surname",
            "disabled": true
          },
          "Role": {
            "renderer": {
              "type": "Role",
              "renderer": "union",
              "cases": {
                "SuperAdmin": "unit",
                "Admin": "unit",
                "Regular": "unit"
              }
            },
            "label": "Role"
          },
          "Job": {
            "renderer": "readonlyOption",
            "noneRenderer": {
              "renderer": "unit",
              "label": "No Job"
            },
            "someRenderer": {
              "renderer": "one",
              "detailsRenderer": {
                "renderer": {
                  "type": "Job",
                  "fields": {
                    "Name": {
                      "renderer": "string",
                      "label": "Name"
                    },
                    "Description": {
                      "renderer": "string",
                      "label": "Description"
                    },
                    "Salary": {
                      "renderer": "float64",
                      "label": "Salary"
                    },
                    "Company": {
                      "renderer": "streamSingleselection",
                      "stream": [
                        "User",
                        "UserCompaniesApi"
                      ],
                      "label": "Company"
                    }
                  },
                  "tabs": {
                    "main": {
                      "columns": {
                        "main": {
                          "groups": {
                            "main": [
                              "Name",
                              "Description",
                              "Salary",
                              "Company"
                            ]
                          }
                        }
                      }
                    }
                  }
                }
              },
              "previewRenderer": {
                "renderer": {
                  "type": "Job",
                  "fields": {
                    "Name": {
                      "renderer": "string",
                      "label": "Name"
                    }
                  },
                  "tabs": {
                    "main": {
                      "columns": {
                        "main": {
                          "groups": {
                            "main": [
                              "Name"
                            ]
                          }
                        }
                      }
                    }
                  }
                }
              },
              "api": [
                "User",
                "UserJobApi"
              ],
              "label": "Job"
            }
          },
          "Team": {
            "renderer": "one",
            "detailsRenderer": {
              "renderer": "team"
            },
            "api": [
              "User",
              "UserTeamApi"
            ],
            "label": "Team"
          },
          "UserGroups": {
            "renderer": "many",
            "detailsRenderer": {
              "renderer": {
                "type": "UserGroup",
                "fields": {
                  "Name": {
                    "renderer": "string",
                    "label": "Name",
                    "disabled": true
                  },
                  "Description": {
                    "renderer": "string",
                    "label": "Name",
                    "disabled": true
                  }
                },
                "tabs": {
                  "main": {
                    "columns": {
                      "main": {
                        "groups": {
                          "main": [
                            "Name",
                            "Description"
                          ]
                        }
                      }
                    }
                  }
                }
              }
            },
            "previewRenderer": {
              "renderer": {
                "type": "UserGroup",
                "fields": {
                  "Name": {
                    "renderer": "string",
                    "label": "Name",
                    "disabled": true
                  }
                },
                "tabs": {
                  "main": {
                    "columns": {
                      "main": {
                        "groups": {
                          "main": [
                            "Name"
                          ]
                        }
                      }
                    }
                  }
                }
              }
            },
            "api": [
              "User",
              "UserGroupsApi"
            ],
            "label": "User Groups"
          },
          "Activities": {
            "renderer": {
              "type": "Activity",
              "renderer": "table",
              "columns": {
                "Log": {
                  "renderer": "string",
                  "label": "Name"
                },
                "Timestamp": {
                  "renderer": "date",
                  "label": "Timestamp"
                }
              },
              "visibleColumns": [
                "Log",
                "Timestamp"
              ]
            },
            "api": [
              "User",
              "UserActivitiesApi"
            ],
            "label": "User Groups"
          }
        },
        "tabs": {
          "main": {
            "columns": {
              "main": {
                "groups": {
                  "main": [
                    "Name",
                    "Surname",
                    "Role",
                    "Team",
                    "Job",
                    "UserGroups",
                    "Activities"
                  ]
                }
              }
            }
          }
        }
      },
      "visibleColumns": [
        "Email"
      ]
    },
    "users": {
      "type": "Users",
      "fields": {
        "Active": {
          "renderer": "activeUsers",
          "api": "ActiveUsersApi",
          "label": "Active Users"
        },
        "Inactive": {
          "renderer": "inactiveUsers",
          "api": "InactiveUsersApi",
          "label": "Inactive Users"
        }
      },
      "tabs": {
        "main": {
          "columns": {
            "main": {
              "groups": {
                "main": [
                  "Active",
                  "Inactive"
                ]
              }
            }
          }
        }
      }
    }
  },
  "launchers": {
    "users": {
      "kind": "passthrough",
      "form": "users",
      "configType": "GlobalConfiguration"
    }
  }
}